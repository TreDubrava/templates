{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"lib": ["ES2022"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"moduleResolution": "bundler",
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"types": [
			"./worker-configuration.d.ts",
		]
	},
	"include": ["src/**/*", "./worker-configuration.d.ts"],
	"exclude": ["node_modules", "dist", ".wrangler"]
}
